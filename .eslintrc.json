{
  "extends": "airbnb",
  "globals": {
    "jest": false,
    "expect": false,
    "describe": false,
    "test": false,
    "afterAll": false
  },
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "react/prop-types": [0]
  }
}